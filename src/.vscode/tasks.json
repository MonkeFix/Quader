{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--bin",
				"quader"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "Run Quader",
			"group": {
